{
  "name": "Bookmarks First",
  "version": "1.1",
  "description": "A new tab page that's all about bookmarks.",
  "author": "Brandon Edgren",
  "permissions": [
      "bookmarks",
      "chrome://favicon/"
  ],
  "chrome_url_overrides": {
      "newtab": "newtab.html"
  },
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
